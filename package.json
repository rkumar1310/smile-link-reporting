{
  "name": "smile-link-nlg",
  "version": "1.0.0",
  "description": "NLG Report Generation System for Smile-Link",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx --env-file=.env watch src/index.ts",
    "build": "tsc",
    "start": "node --env-file=.env dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "tsc --noEmit",
    "reports:generate": "tsx --env-file=.env scripts/regenerate-reports.ts"
  },
  "keywords": [
    "nlg",
    "dental",
    "report-generation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.12",
    "ai": "^6.0.31",
    "typescript": "^5.9.3",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsx": "^4.21.0",
    "vitest": "^4.0.16"
  }
}
